	<!--	Schema OUTLINE
		The contents of the schema are organized as follows:
			SCHEMA REVISION HISTORY
			IMPORT OF OTHER NEEDED SCHEMA (call for special characters, etc.)
			RDA RECORD & ITS CHILDREN, with annotations
				frbrWork
				frbrExpression
				frbrManifestation
				frbrItem
				frbrPerson
				frbrFamily
				frbrCorporateBody
				frbrConcept
				frbrObject
				frbrEvent
				frbrPlace
				fradName
				frbrRelationships
			RDA ELEMENTS & THIER CHILDREN (i.e., element types referenced in RDA Record)
				rdaWork
				rdaExpression
				rdaManifestation
				rdaItem
				rdaPerson
				rdaFamily
				rdaCorporateBody
				rdaConcept
				rdaObject
				rdaEvent
				rdaPlace
				rdaName
				rdaRelationships
		See RDA ERD available at www.rdaonline.org for a full visual detail of RDA entities and 
		their relationships to FRBR entities.-->

	<!-- ======================================================================= -->
	<!-- SCHEMA REVISION HISTORY
		7-11-2009  Initial Release v1.0 -->
	<!-- ======================================================================= -->

	<!-- ======================================================================= -->
	<!--  IMPORT OF OTHER NEEDED SCHEMA, including special characters, xlinks, & 
		namespaces -->
	<!-- ======================================================================= -->
	<xs:import namespace="http://www.w3.org/1998/Math/MathML"
		schemaLocation="ncbi-mathml2/mathml2.xsd"/>
	<xs:import namespace="http://www.w3.org/1999/xlink" schemaLocation="xlink.xsd"/>
	<xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>


	<!-- ======================================================================= -->
	<!--  RDA RECORD -->
	<!-- ======================================================================= -->
	<xs:element name="RDARecord">
		<xs:complexType>
			<xs:annotation id="rdar-1">
				<xs:documentation>Root element of an RDA record.</xs:documentation>
			</xs:annotation>
			<xs:sequence>
				<xs:element name="frbrWork" minOccurs="0" maxOccurs="1" type="rdaWork">
					<xs:annotation id="rdar-2">
						<xs:documentation>FRBR Group 1: Work Entity</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="frbrExpression" minOccurs="0" maxOccurs="1" type="rdaExpression">
					<xs:annotation id="rdar-3">
						<xs:documentation>FRBR Group 1: Expression Entity</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="frbrManifestation" minOccurs="0" maxOccurs="1" type="rdaManifestation">
					<xs:annotation id="rdar-4">
						<xs:documentation>FRBR Group 1: Manifestation Entity</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="frbrItem" minOccurs="0" maxOccurs="unbounded" type="rdaItem">
					<xs:annotation id="rdar-5">
						<xs:documentation>FRBR Group 1: Item Entity</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="frbrPerson" minOccurs="0" maxOccurs="unbounded" type="rdaPerson">
					<xs:annotation id="rdar-6">
						<xs:documentation>FRBR Group 2: Person Entity</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="frbrFamily" minOccurs="0" maxOccurs="unbounded" type="rdaFamily">
					<xs:annotation id="rdar-7">
						<xs:documentation>FRBR Group 2: Family Entity</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="frbrCorporateBody" minOccurs="0" maxOccurs="unbounded"
					type="rdaCorporateBody">
					<xs:annotation id="rdar-8">
						<xs:documentation>FRBR Group 2: Corporate Body Entity</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="frbrConcept" minOccurs="0" maxOccurs="unbounded" type="rdaConcept">
					<xs:annotation id="rdar-9">
						<xs:documentation>FRBR Group 3: Concept Entity</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="frbrObject" minOccurs="0" maxOccurs="unbounded" type="rdaObject">
					<xs:annotation id="rdar-10">
						<xs:documentation>FRBR Group 3: Object Entity</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="frbrEvent" minOccurs="0" maxOccurs="unbounded" type="rdaEvent">
					<xs:annotation id="rdar-11">
						<xs:documentation>FRBR Group 3: Event Entity</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="frbrPlace" minOccurs="0" maxOccurs="unbounded" type="rdaPlace">
					<xs:annotation id="rdar-12">
						<xs:documentation>FRBR Group 3: Place Entity</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="fradName" minOccurs="0" maxOccurs="1" type="rdaName">
					<xs:annotation id="rdar-13">
						<xs:documentation>FRAD Name Entity</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="frbrRelationships" minOccurs="0" maxOccurs="unbounded"
					type="rdaRelationships">
					<xs:annotation id="rdar-14">
						<xs:documentation>FRBR Relationships</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>


	<!-- ======================================================================= -->
	<!--  RDA WORK -->
	<!-- ======================================================================= -->
	<xs:complexType name="rdaWork">
		<xs:sequence>
			<xs:annotation id="rdar-15">
				<xs:documentation>RDA conceptualization of FRBR Group 1 Work Entity with accompanying RDA
					elements, subelements, and vocabularies. Where RDA defines Work as: "A distinct
					intellectual or artistic creation (i.e., the intellectual or artistic
				content)"</xs:documentation>
			</xs:annotation>
			<xs:element name="rdaCataloguersNote">
				<xs:annotation id="rdar-16">
					<xs:documentation>An annotation that might be helpful to those using or revising the
						authorized access point representing an entity or relationship data, or creating an
						authorized access point representing a related entity.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaCoordinatesOfCartographicContent">
				<xs:annotation id="rdar-17">
					<xs:documentation>A mathematical system for identifying the area covered by the
						cartographic content of a resource. Coordinates may be expressed by means of longitude
						and latitude on the surface of planets or by the angles of right ascension and
						declination for celestial charts.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="rdaLongitudeAndLatitude">
							<xs:annotation id="rdar-18">
								<xs:documentation>A system for identifying the area covered by the cartographic
									content of a resource using longitude of the westernmost and easternmost
									boundaries and latitude of the northernmost and southernmost
								boundaries.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="id" type="xs:ID"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="rdaStringsOfCoordinatePairs">
							<xs:annotation id="rdar-19">
								<xs:documentation>A system for identifying the precise area covered by the
									cartographic content of a resource using coordinates for each vertex of a
								polygon.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="id" type="xs:ID"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="rdaRightAscensionAndDeclination">
							<xs:annotation id="rdar-20">
								<xs:documentation>A system for identifying the location of a celestial object in the
									sky covered by the cartographic content of a resource using the angles of right
									ascension and declination.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="id" type="xs:ID"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaCoverageOfTheContent">
				<xs:annotation id="rdar-21">
					<xs:documentation>The chronological or geographic coverage of the content of a
					resource.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaDateOfWork">
				<xs:annotation id="rdar-22">
					<xs:documentation>The earliest date associated with a work.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaDissertationorThesisInformation">
				<xs:annotation id="rdar-23">
					<xs:documentation>Information relating to a work presented as part of the formal
						requirements for an academic degree.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="rdaYearDegreeGranted">
							<xs:annotation id="rdar-24">
								<xs:documentation>The calendar year in which a granting institution or faculty
									conferred an academic degree on a candidate.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="id" type="xs:ID"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="rdaGrantingInstitutionorFaculty">
							<xs:annotation id="rdar-25">
								<xs:documentation>An institution or faculty conferring an academic degree on a
									candidate.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="id" type="xs:ID"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="rdaAcademicDegree">
							<xs:annotation id="rdar-26">
								<xs:documentation>A rank conferred as a guarantee of academic
								proficiency.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="id" type="xs:ID"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaEpoch">
				<xs:annotation id="rdar-27">
					<xs:documentation>An arbitrary moment in time to which measurements of position for a body
						or orientation for an orbit are referred.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaEquinox">
				<xs:annotation id="rdar-28">
					<xs:documentation>One of two points of intersection of the ecliptic and the celestial
						equator, occupied by the sun when its declination is 0Â°.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaFormOfWork">
				<xs:annotation id="rdar-29">
					<xs:documentation>A class or genre to which a work belongs.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaHistoryOfTheWork">
				<xs:annotation id="rdar-30">
					<xs:documentation>Information pertaining to the history of a work.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaIdentifierForTheWork">
				<xs:annotation id="rdar-31">
					<xs:documentation>A character string uniquely associated with a work, or with a surrogate
						for a work (e.g., an authority record), that serves to differentiate that work from
						other works.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaIntendedAudience">
				<xs:annotation id="rdar-32">
					<xs:documentation>The class of user for which the content of a resource is intended, or
						for whom the content is considered suitable, as defined by age group (e.g., children,
						young adults, adults, etc.), educational level (e.g., primary, secondary, etc.), type of
						disability, or other categorization.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaKey">
				<xs:annotation id="rdar-33">
					<xs:documentation>The set of pitch relationships that establishes the tonal centre, or
						principal tonal centre, for a musical work. Key is designated by its pitch name and its
						mode, when it is major or minor.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaMediumOfPerformance">
				<xs:annotation id="rdar-34">
					<xs:documentation>The instrument, instruments, voice, voices, etc., for which a musical
						work was originally conceived.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="RDAMediumOfPerformance">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="AccompanimentForSongsLiederetc_Accompanied"/>
										<xs:enumeration value="AccompanimentForSongsLiederetc_Accompanied"/>
										<xs:enumeration value="AccompanimentForSongsLiederetc_Unaccompanied"/>
										<xs:enumeration value="Choruses_MensVoices"/>
										<xs:enumeration value="Choruses_MixedVoices"/>
										<xs:enumeration value="Choruses_UnisonVoices"/>
										<xs:enumeration value="Choruses_WomensVoices"/>
										<xs:enumeration value="GroupsOfInstruments_Brasses"/>
										<xs:enumeration value="GroupsOfInstruments_Instrumental[stringwindetc.]Ensemble"/>
										<xs:enumeration value="GroupsOfInstruments_KeyboardInstrument"/>
										<xs:enumeration value="GroupsOfInstruments_Percussion"/>
										<xs:enumeration value="GroupsOfInstruments_PluckedInstrument"/>
										<xs:enumeration value="GroupsOfInstruments_Strings"/>
										<xs:enumeration value="GroupsOfInstruments_Winds"/>
										<xs:enumeration value="GroupsOfInstruments_Woodwinds"/>
										<xs:enumeration value="IndividualInstruments_Continuo"/>
										<xs:enumeration value="IndividualInstruments_KeyboardInstruments"/>
										<xs:enumeration value="IndividualInstruments_Organs(2)"/>
										<xs:enumeration value="IndividualInstruments_Piano"/>
										<xs:enumeration value="IndividualInstruments_Piano4Hands"/>
										<xs:enumeration value="IndividualInstruments_Pianos(2)"/>
										<xs:enumeration value="IndividualInstruments_Pianos(2)8Hands"/>
										<xs:enumeration value="Orchestraetc._Band"/>
										<xs:enumeration value="Orchestraetc._Orchestra"/>
										<xs:enumeration value="Orchestraetc._StringOrchestra"/>
										<xs:enumeration value="SoloVoices_Alto"/>
										<xs:enumeration value="SoloVoices_Baritone"/>
										<xs:enumeration value="SoloVoices_Bass"/>
										<xs:enumeration value="SoloVoices_Mezzo-Soprano"/>
										<xs:enumeration value="SoloVoices_Soprano"/>
										<xs:enumeration value="SoloVoices_Tenor"/>
										<xs:enumeration value="StandardCombination_PianoStrings">
											<xs:annotation id="rdar-50">
												<xs:documentation>Standard combination: piano trio; instrumentation: piano,
													violin, violoncello. Standard combination: piano quartet; instrumentation:
													piano, violin, viola, violoncello. Standard combination: piano quintet;
													instrumentation: piano, 2 violins, viola, violoncello.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="StandardCombination_Strings">
											<xs:annotation id="rdar-51">
												<xs:documentation>Standard combination: string trio; instrumentation:
													violin, viola, violoncello. Standard combination: string quartet;
													instrumentation: 2 violins, viola, violoncello.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="StandardCombination_Winds">
											<xs:annotation id="rdar-52">
												<xs:documentation>Standard combination: woodwind quartet; instrumentation:
													flute, oboe, clarinet, horn, bassoon.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="StandardCombination_Woodwinds">
											<xs:annotation id="rdar-53">
												<xs:documentation>Standard combination: flute, oboe, clarinet,
												bassoon.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="TwoorMoreSoloVoices_MensSoloVoices"/>
										<xs:enumeration value="TwoorMoreSoloVoices_MixedSoloVoices"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute name="id" type="xs:ID"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaNatureOfTheContent">
				<xs:annotation id="rdar-35">
					<xs:documentation>The specific character of the primary content of a resource (e.g., legal
						articles, interim report).</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaNumericDesignationOfAMusicalWork">
				<xs:annotation id="rdar-36">
					<xs:documentation>A serial number, opus number, or thematic index number assigned to a
						musical work by a composer, publisher, or a musicologist.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaOtherDistinguishingCharacteristicOfTheWork">
				<xs:annotation id="rdar-37">
					<xs:documentation>A characteristic other than form of work, date of work, or place of
						origin of the work that serves to differentiate a work from another work with the same
						title or from the name of a person, family, or corporate body.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaPlaceOfOriginOfTheWork">
				<xs:annotation id="rdar-38">
					<xs:documentation>The country or other territorial jurisdiction from which a work
						originated.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaSignatorytoATreatyetc.">
				<xs:annotation id="rdar-39">
					<xs:documentation>A government or other party that has formally signed a treaty, etc., as
						an adherent to its terms and conditions.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaSourceConsulted">
				<xs:annotation id="rdar-40">
					<xs:documentation>A resource used in determining the name, title, or other identifying
						attributes of an entity, or in determining the relationship between
					entities.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaStatusOfIdentification">
				<xs:annotation id="rdar-41">
					<xs:documentation>An indication of the level of authentication of the data identifying an
						entity.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="RDAStatusOfIdentification">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="FullyEstablished"/>
										<xs:enumeration value="Preliminary"/>
										<xs:enumeration value="Provisional"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute name="id" type="xs:ID"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaSystemOfOrganization">
				<xs:annotation id="rdar-42">
					<xs:documentation>A system of arranging materials in an archival resource or a
					collection.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaTitleOfWork">
				<xs:annotation id="rdar-43">
					<xs:documentation>A word, character, or group of words and/or characters by which a work
						is known.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="rdaPreferredTitleForTheWork">
							<xs:annotation id="rdar-44">
								<xs:documentation>The title or form of title chosen as the basis for the authorized
									access point representing that work.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="id" type="xs:ID"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="rdaVariantTitleForTheWork">
							<xs:annotation id="rdar-45">
								<xs:documentation>A title or form of title by which a work is known that differs
									from the title or form of title chosen as the preferred title for the
								work.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="id" type="xs:ID"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="id" type="xs:ID"/>
	</xs:complexType>

	<!-- ======================================================================= -->
	<!--  RDA EXPRESSION -->
	<!-- ======================================================================= -->
	<xs:complexType name="rdaExpression">
		<xs:sequence>
			<xs:annotation>
				<xs:documentation>RDA conceptualization of FRBR Group 1 Expression Entity with accompanying
					RDA elements, subelements, and vocabularies. Where RDA defines Expression as: "The
					intellectual or artistic realization of a work in the form of alpha-numeric, musical or
					choreographic notation, sound, image, object, movement, etc., or any combination of such
					forms."</xs:documentation>
			</xs:annotation>
			<xs:element name="rdaAccessibilityContent">
				<xs:annotation id="rdar-54">
					<xs:documentation>Content that assists those with a sensory impairment in the greater
						understanding of content which their impairment prevents them fully seeing or
					hearing.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaArtisticAndorTechnicalCredit">
				<xs:annotation id="rdar-55">
					<xs:documentation>A listing of persons, families, or corporate bodies making contributions
						to the artistic and/or technical production of a motion picture or video recording other
						than as performers, narrators, or presenters.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaAspectRatio">
				<xs:annotation id="rdar-56">
					<xs:documentation>The ratio of the width to the height of a moving
					image.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="RDAAspectRatio">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="RDAFullScreen"/>
										<xs:enumeration value="RDAMixed"/>
										<xs:enumeration value="RDAWideScreen"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute name="id" type="xs:ID"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaCataloguersNote">
				<xs:annotation id="rdar-57">
					<xs:documentation>An annotation that might be helpful to those using or revising the
						authorized access point representing an entity or relationship data, or creating an
						authorized access point representing a related entity.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaColourContent">
				<xs:complexType>
					<xs:annotation id="rdar-58">
						<xs:documentation>The presence of colour, tone, etc., in the content of a resource.
							Colour content is also the specific colours, tones, etc., (including black and white)
							present in the content of a resource.</xs:documentation>
					</xs:annotation>
					<xs:sequence>
						<xs:element name="rdaColorOfThree-DimensionalForm">
							<xs:annotation id="rdar-59">
								<xs:documentation>The presence of colour, tone, etc., in a three-dimensional form or
									forms contained in a resource. Colour of three-dimensional form is also the
									specific colours, tones, etc., (including black and white) present in a
									three-dimensional form or forms contained in a resource.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:string">
										<xs:attribute name="RDAColorOfThree-DimensionalForm">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="BlackAndWhite"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:attribute>
										<xs:attribute name="id" type="xs:ID"/>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="rdaColourOfContentOfResourceDesignedForPersonsWIthVisualImpairments">
							<xs:annotation id="rdar-60">
								<xs:documentation>The presence of colour, tone, etc., in a resource designed for
									persons with visual impairments. Colour content of a resource designed for persons
									with visual impairments is also the specific colours, tones, etc., (excluding
									black and white) present in a resource designed for persons with visual
									impairments.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="rdaColourOfMovingImage">
							<xs:annotation id="rdar-61">
								<xs:documentation>The presence of colour, tone, etc., in a moving image or images
									contained in a resource. Colour of moving image is also the specific colours,
									tones, etc., (including black and white) present in a moving image or images
									contained in a resource.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:string">
										<xs:attribute name="RDAColourOfMovingImage">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="BlackAndWhite"/>
													<xs:enumeration value="Sepia"/>
													<xs:enumeration value="Tinted"/>
													<xs:enumeration value="TintedAndToned"/>
													<xs:enumeration value="Toned"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:attribute>
										<xs:attribute name="id" type="xs:ID"/>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="rdaColourOfStillImage">
							<xs:annotation id="rdar-62">
								<xs:documentation>The presence of colour, tone, etc., in a still image or images
									contained in a resource. Colour of still image is also the specific colours,
									tones, etc., (including black and white) present in a still image or images
									contained in a resource.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:string">
										<xs:attribute name="RDAColourOfStillImage">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="BlackAndWhite"/>
													<xs:enumeration value="GrayScale"/>
													<xs:enumeration value="Sepia"/>
													<xs:enumeration value="Tinted"/>
													<xs:enumeration value="TintedAndToned"/>
													<xs:enumeration value="Toned"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:attribute>
										<xs:attribute name="id" type="xs:ID"/>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaContentType">
				<xs:annotation id="rdar-63">
					<xs:documentation>A categorization reflecting the fundamental form of communication in
						which the content is expressed and the human sense through which it is intended to be
						perceived. For content expressed in the form of an image or images, content type also
						reflects the number of spatial dimensions in which the content is intended to be
						perceived and the perceived presence or absence of movement.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="RDAContentType">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="CartographicDataset">
											<xs:annotation id="rdar-64">
												<xs:documentation>Cartographic content expressed through a digitally encoded
													dataset intended to be processed by a computer. For cartographic data
													intended to be perceived in the form of an image or three-dimensional
													form, see Cartographic Image, Cartographic Moving Image, Cartographic
													Tactile Image, Cartographic Tactile Three-Dimensional Form, and
													Cartographic Three-Dimensional Form.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="CartographicImage">
											<xs:annotation id="rdar-65">
												<xs:documentation>Cartographic content expressed through line, shape,
													shading, etc., intended to be perceived visually as a still image or
													images in two dimensions. Includes maps, views, atlases, remote-sensing
													images, etc.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="CartographicMovingImage">
											<xs:annotation id="rdar-66">
												<xs:documentation>Cartographic content expressed through images intended to
													be perceived as moving, in two dimensions. Includes satellite images of
													the Earth or other celestial bodies in motion.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="CartographicTactileImage">
											<xs:annotation id="rdar-67">
												<xs:documentation>Cartographic content expressed through line, shape, and/or
													other forms, intended to be perceived through touch as a still image in
													two dimensions.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="CartographicTactileThree-DimensionalForm">
											<xs:annotation id="rdar-68">
												<xs:documentation>Cartographic content expressed through a form or forms
													intended to be perceived through touch as a three-dimensional form or
													forms.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="CartographicThree-DimensionalForm">
											<xs:annotation id="rdar-69">
												<xs:documentation>Cartographic content expressed through a form or forms
													intended to be perceived visually in three-dimensions. Includes globes,
													relief models, etc.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="ComputerDataset">
											<xs:annotation id="rdar-70">
												<xs:documentation>Content expressed through a digitally encoded dataset
													intended to be processed by a computer. Includes numeric data,
													environmental data, etc., used by applications software to calculate
													averages, correlations, etc., or to produce models, etc., but not normally
													displayed in its raw form.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="ComputerProgram">
											<xs:annotation id="rdar-71">
												<xs:documentation>Content expressed through digitally encoded instructions
													intended to be processed and performed by a computer. Includes operating
													systems, applications software, etc.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="NotatedMovement">
											<xs:annotation id="rdar-72">
												<xs:documentation>Content expressed through a form of notation for movement
													intended to be perceived visually. Includes all forms of movement
													notation, notation other than those intended to be perceived through
													touch.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="NotatedMusic">
											<xs:annotation id="rdar-73">
												<xs:documentation>Content expressed through a form of musical notation
													intended to be perceived visually. Includes all forms of musical notation,
													other than those intended to be perceived through
												touch.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="Other">
											<xs:annotation id="rdar-74">
												<xs:documentation/>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="PerformedMusic">
											<xs:annotation id="rdar-75">
												<xs:documentation>Content expressed through music in an audible form.
													Includes recorded performances of music, computer-generated music,
												etc.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="Sounds">
											<xs:annotation id="rdar-76">
												<xs:documentation>Content other than language or music, expressed in an
													audible form. Includes natural sounds, artificially produced sounds,
												etc.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="SpokenWord">
											<xs:annotation id="rdar-77">
												<xs:documentation>Content expressed through language in an audible form.
													Includes recorded readings, recitations, speeches, interviews, oral
													histories, etc., computergenerated speech, etc.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="StillImage">
											<xs:annotation id="rdar-78">
												<xs:documentation>Content expressed through line, shape, shading, etc.,
													intended to be perceived visually in two dimensions. Includes drawings,
													paintings, diagrams, photographic images (stills), etc.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="TactileImage">
											<xs:annotation id="rdar-79">
												<xs:documentation>Content expressed through line, shape, and/or other forms,
													intended to be perceived through touch as a still image in two
												dimensions.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="TactileNotatedMovement">
											<xs:annotation id="rdar-80">
												<xs:documentation>Content expressed through a form of notation for movement
													intended to be perceived through touch.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="TactileNotatedMusic">
											<xs:annotation id="rdar-81">
												<xs:documentation>Content expressed through a form of musical notation
													intended to be perceived through touch. Includes braille music and other
													tactile forms of musical notation.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="TactileText">
											<xs:annotation id="rdar-82">
												<xs:documentation>Content expressed through a form of notation for language
													intended to be perceived through touch. Includes braille text and other
													tactile forms of language notation.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="TactileThree-DimensionalForm">
											<xs:annotation id="rdar-83">
												<xs:documentation>Content expressed though a form or forms intended to be
													perceived through touch as a three- dimensional form or
												forms.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="Text">
											<xs:annotation id="rdar-84">
												<xs:documentation>Content expressed through a form of notation for language
													intended to be perceived visually. Includes all forms of language notation
													other than those intended to be perceived though touch.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="Three-DimensionalForm">
											<xs:annotation id="rdar-85">
												<xs:documentation>Content expressed through a form or forms intended to be
													perceived visually in three-dimensions. Includes sculptures, models,
													naturally occurring objects and specimens, holograms,
												etc.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="Three-DimensionalMovingImage">
											<xs:annotation id="rdar-86">
												<xs:documentation>Content expressed through images intended to be perceived
													as moving, in three-dimensions. Includes 3-D motion pictures (using live
													action and/or animation), 3-D video games, etc. Three-dimensional moving
													images may or may not be accompanied by sound.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="Two-DimensionalMovingImage">
											<xs:annotation id="rdar-87">
												<xs:documentation>Content expressed through images intended to be perceived
													as moving, in two dimensions. Includes motion pictures (using live action
													and/or animation), film and video recordings of performances, events,
													etc., other than those intended to be perceived in three dimensions.
													Moving images may or may not be accompanied by sound.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="Unspecified">
											<xs:annotation id="rdar-88">
												<xs:documentation/>
											</xs:annotation>
										</xs:enumeration>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute name="id" type="xs:ID"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaDateOfExpression">
				<xs:annotation id="rdar-89">
					<xs:documentation>The earliest date associated with an expression.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaDuration">
				<xs:annotation id="rdar-90">
					<xs:documentation>The playing time, running time, etc., of the content of a resource.
						Duration also includes performance time for a resource containing notated music or
						notated movement.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaFormOfNotation">
				<xs:annotation id="rdar-91">
					<xs:documentation>A set of characters and/or symbols used to express the content of a
						resource.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="rdaFormOfNotatedMovement">
							<xs:annotation id="rdar-92">
								<xs:documentation>A set of characters and/or symbols used to express the movement
									content of a resource.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:string">
										<xs:attribute name="RDAFormOfNotatedMovement">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="ActionStrokeDanceNotation"/>
													<xs:enumeration value="Beauchamp-FeuilletNotation"/>
													<xs:enumeration value="BeneshMovementNotation"/>
													<xs:enumeration value="DanceWriting"/>
													<xs:enumeration value="Eshkol-WochmanMovementNotation"/>
													<xs:enumeration value="GamePlayNotation"/>
													<xs:enumeration value="KinetographyLaban"/>
													<xs:enumeration value="Labanotation"/>
													<xs:enumeration value="StepanoyDanceNotation"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:attribute>
										<xs:attribute name="id" type="xs:ID"/>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="rdaaFormOfMusicalNotation">
							<xs:annotation id="rdar-93">
								<xs:documentation>A set of characters and/or symbols used to express the musical
									content of a resource.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:string">
										<xs:attribute name="RDAFormOfMusicalNotation">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="GraphicNotation">
														<xs:annotation id="rdar-135">
															<xs:documentation>A form of musical notation that uses various
																suggestive lines, symbols, colour, etc., to prompt or guide the
																performers. It is used for music that is indeterminate in pitch,
																duration, temperament, etc., and also to depict electronic music in
																which no performer is involved.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="LetterNotation">
														<xs:annotation id="rdar-136">
															<xs:documentation>Musical notation that uses the letters of the
																alphabet to designate pitches.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="MensuralNotation">
														<xs:annotation id="rdar-137">
															<xs:documentation>A system of notating duration, beginning around 1260
																and continuing through about 1600, employing four principal
																note-values and associated rests: long, breve, semibreve, and
															minim.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="NeumatieNotation">
														<xs:annotation id="rdar-138">
															<xs:documentation>A system of musical notation using neumes, i.e.,
																graphic signs that represent essentially the movement in pitch of a
																melody.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="NumberNotation">
														<xs:annotation id="rdar-139">
															<xs:documentation>A system of musical notation conveying pitch by use
																of numbers, assigned to the notes of a scale, the keys of a
																keyboard, the finger positions or frets of a string instrument, or
																to the holes or valves of a wind instrument.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="Solmization">
														<xs:annotation id="rdar-140">
															<xs:documentation>The designation of pitches by means of conventional
																syllables rather than letter names.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="StaffNotation">
														<xs:annotation id="rdar-141">
															<xs:documentation>A system of musical notation in wide use for Western
																art music, conveying pitch and duration using a staff of multiple
																parallel lines (in 15th- to 21st-century music, usually five lines)
																often in combination with other staves.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="Tablature">
														<xs:annotation id="rdar-142">
															<xs:documentation>Any notational system from 1300 or later that uses
																letters, numbers or other signs as an alternative to conventional
																staff notation.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="TonicSol-fa">
														<xs:annotation id="rdar-143">
															<xs:documentation>A system of musical notation that replaces staff
																notation with sol-fa syllables or their initials.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
												</xs:restriction>
											</xs:simpleType>
										</xs:attribute>
										<xs:attribute name="id" type="xs:ID"/>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="rdaFormOfTactileNotation">
							<xs:annotation id="rdar-94">
								<xs:documentation>A set of characters and/or symbols used to express the content of
									a resource in a form that can be perceived through touch.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:string">
										<xs:attribute name="RDAFormOfTactileNotation">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="BrailleCode">
														<xs:annotation id="rdar-146">
															<xs:documentation>A form of tactile notation for text using embossed
																characters formed by raised dots in six-dot
															cells.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="ComputingBrailleCode"/>
													<xs:enumeration value="MathematicalBrailleCode"/>
													<xs:enumeration value="ModeCode">
														<xs:annotation id="rdar-147">
															<xs:documentation>A tactile alphabet based on simplified letter forms,
																for use by the visually impaired.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="MusicBrailleCode"/>
													<xs:enumeration value="TactileGraphic"/>
													<xs:enumeration value="TactileMusicalNotation"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:attribute>
										<xs:attribute name="id" type="xs:ID"/>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="Script">
							<xs:annotation id="rdar-95">
								<xs:documentation>A set of characters and/or symbols used to express the written
									language content of a resource.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="id" type="xs:ID"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaFormatOfNotatedMusic">
				<xs:annotation id="rdar-96">
					<xs:documentation>The musical or physical layout of the content of a resource that is
						presented in the form of musical notation.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="RDAFormatOfNotatedMusic">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="ChoirBook">
											<xs:annotation id="rdar-97">
												<xs:documentation>A large music book made to be placed on a stand in front
													of a choir. Each part is notated separately, usually in the configuration
													that presents, when the book is open, the soprano and tenor parts on the
													verso of a leaf, and the alto and bass parts on the recto of the next
													leaf.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="ChorusScore">
											<xs:annotation id="rdar-98">
												<xs:documentation>A score of a work for solo voices and chorus showing only
													the parts for chorus, at least in those portions of the work in which the
													chorus sings, with the instrumental accompaniment either arranged for one
													or two keyboard instruments or omitted.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="CondensedScore">
											<xs:annotation id="rdar-99">
												<xs:documentation>A score in which the number of staves is reduced to two or
													a few, generally organized by instrumental sections, and often with cues
													for individual parts. Used for: Reduced score, Short
												score.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="Part">
											<xs:annotation id="rdar-100">
												<xs:documentation>One of the units into which a resource has been divided by
													the publisher, manufacturer, etc. It is distinguished from a fascicle by
													being a formal component unit rather than a temporary division of a
													resource. 2. In the context of notated music, a component consisting of
													the music for the use of one or more, but not all,
												performers.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="PianoConductorPart">
											<xs:annotation id="rdar-101">
												<xs:documentation>A performance part for a piano performer in an ensemble,
													with cues for the other instruments that enable the performer of that part
													also to conduct.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="PianoScore">
											<xs:annotation id="rdar-102">
												<xs:documentation>A reduction of an instrumental work or a vocal work with
													instruments to a version for piano. May include the words of a vocal
												work.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="Score">
											<xs:annotation id="rdar-103">
												<xs:documentation>Graphical, symbolic, or word-based musical notation
													representing the sounds of all the parts of an ensemble or a work for solo
													performer or electronic media. Do not confuse with
												Part.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="StudyScore">
											<xs:annotation id="rdar-104">
												<xs:documentation>A score issued in a musical image of reduced size, not
													primarily intended for use in performance. A descriptive phrase such as
													"Study score", "Miniature score", "Taschenpartitur", "Partition de poche",
													etc., usually appears on the resource.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="TableBook">
											<xs:annotation id="rdar-105">
												<xs:documentation>A music book made to be placed on a table and displayed in
													such a way that the performers can read their parts while seated or
													standing across or around the table. Each part is notated separately,
													usually in a configuration that presents, when the book is open, different
													parts in inverted and/or perpendicular positions.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="ViolinConductorPart">
											<xs:annotation id="rdar-106">
												<xs:documentation>A performance part for a violin performer in an ensemble,
													with cues for the other instruments that enable the performer of that part
													also to conduct.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="VocalScore">
											<xs:annotation id="rdar-107">
												<xs:documentation>A score showing all vocal parts, with accompaniment
													arranged for one or two keyboard instruments.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute name="id" type="xs:ID"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaIdentifierForTheExpression">
				<xs:annotation id="rdar-108">
					<xs:documentation>A character string uniquely associated with an expression, or with a
						surrogate for an expression (e.g., an authority record), that serves to differentiate
						that expression from other expressions.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaIllustrativeContent">
				<xs:annotation id="rdar-109">
					<xs:documentation>Content designed to illustrate the primary content of a
					resource.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="RDAIllustrativeContent">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Charts">
											<xs:annotation id="rdar-110">
												<xs:documentation>1. Two-dimensional representations of data in graphic or
													tabular form (e.g., a wall chart). 2. Maps designed primarily for
													navigation through water, air, or space.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="CoatsOfArms"/>
										<xs:enumeration value="Facsimilies">
											<xs:annotation id="rdar-148">
												<xs:documentation>Reproductions simulating the physical appearance of the
													original in addition to reproducing its content
												exactly.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="Forms"/>
										<xs:enumeration value="GenealogicalTables"/>
										<xs:enumeration value="Graphs"/>
										<xs:enumeration value="Illuminations"/>
										<xs:enumeration value="Illustration"/>
										<xs:enumeration value="Illustrations"/>
										<xs:enumeration value="Maps">
											<xs:annotation id="rdar-111">
												<xs:documentation>Representations, normally to scale and on a
													two-dimensional medium, of a selection of material or abstract features
													on, or in relation to, the surface of Earth, another celestial body, or an
													imaginary place. See also Cartographic image, Chart .</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="Music"/>
										<xs:enumeration value="Photographs"/>
										<xs:enumeration value="Plans"/>
										<xs:enumeration value="Portraits"/>
										<xs:enumeration value="Samples"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute name="id" type="xs:ID"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaLanguageOfExpression">
				<xs:annotation id="rdar-112">
					<xs:documentation>A language in which a work is expressed.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaLanguageOfTheContent">
				<xs:annotation id="rdar-113">
					<xs:documentation>A language used to express the content of a resource.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaMediumOfPerformanceOfMusicalContent">
				<xs:annotation id="rdar-114">
					<xs:documentation>The instrument, instruments, voice, voices, etc., used (or intended to
						be used) for performance of musical content.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaOtherDetailsOfCartographicContent">
				<xs:annotation id="rdar-115">
					<xs:documentation>Mathematical data and other features of the cartographic content of a
						resource not recorded in statements of scale, projection, and
					coordinates.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaOtherDistinguishingCharacteristicOfTheExpression">
				<xs:annotation id="rdar-116">
					<xs:documentation>A characteristic other than content type, language of expression, or
						date of expression that serves to differentiate an expression from another expression of
						the same work.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="RDAOtherDistinguishingCharacteristicOfTheExpression">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Selections"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute name="RDAOtherDistinguishingCharacteristicOfTheExpressionOfALegalWork">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Protocolsetc."/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute name="RDAOtherDistinguishingCharacteristicOfTheExpressionOfAMusicalWork">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Arranged"/>
										<xs:enumeration value="ChorusScores"/>
										<xs:enumeration value="Librettos"/>
										<xs:enumeration value="Sketches"/>
										<xs:enumeration value="Text"/>
										<xs:enumeration value="Texts"/>
										<xs:enumeration value="VocalScores"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute
								name="RDAOtherDistinguishingCharacteristicOfTheExpressionOfAReligiousWork">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Confraternity"/>
										<xs:enumeration value="Douai"/>
										<xs:enumeration value="Selections"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute name="id" type="xs:ID"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaPerformerNarratorAndorPresenter">
				<xs:annotation id="rdar-117">
					<xs:documentation>A person, family, or corporate body responsible for performing,
						narrating, and/or presenting a work.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaPlaceAndDateOfCapture">
				<xs:annotation id="rdar-118">
					<xs:documentation>The place and date associated with the capture (i.e., recording,
						filming, etc.) of the content of a resource.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="rdaDateOfCapture">
							<xs:annotation id="rdar-119">
								<xs:documentation>A date or range of dates associated with the capture (i.e.,
									recording, filming, etc.) of the content of a resource.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="id" type="xs:ID"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="rdaPlaceOfCapture">
							<xs:annotation id="rdar-120">
								<xs:documentation>The place associated with the capture (i.e., recording, filming,
									etc.) of the content of a resource.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="id" type="xs:ID"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaProjectionOfCartographicContent">
				<xs:annotation id="rdar-121">
					<xs:documentation>The method or system used to represent the surface of the Earth or of a
						celestial sphere on a plane.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaScale">
				<xs:annotation id="rdar-122">
					<xs:documentation>The ratio of the dimensions of an image or three-dimensional form
						contained or embodied in a resource to the dimensions of the entity it
					represents.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="rdaAdditionalScaleInformation">
							<xs:annotation id="rdar-123">
								<xs:documentation>Supplemental information pertaining to scale such as a statement
									of comparative measures or limitation of the scale to particular parts of the
									content of a resource.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="id" type="xs:ID"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="rdaHorizontalScaleOfCartographicContent">
							<xs:annotation id="rdar-124">
								<xs:documentation>The ratio of horizontal distances in the cartographic content of a
									resource to the actual distances they represent.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="id" type="xs:ID"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="rdaScaleOfStillImageorThree-DimensionalForm">
							<xs:annotation id="rdar-125">
								<xs:documentation>The ratio of the dimensions of a still image or three-dimensional
									form contained or embodied in a resource to the dimensions of the entity it
									represents.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="id" type="xs:ID"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="rdaVerticalScaleOfCartographicContent">
							<xs:annotation id="rdar-126">
								<xs:documentation>The scale of elevation or vertical dimension of the cartographic
									content of a resource.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="id" type="xs:ID"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaSoundContent">
				<xs:annotation id="rdar-127">
					<xs:documentation>The presence of sound in a resource other than one that consists
						primarily of recorded sound.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="RDASoundContent">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Silent"/>
										<xs:enumeration value="Sound"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute name="id" type="xs:ID"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaSourceConsulted">
				<xs:annotation id="rdar-128">
					<xs:documentation>A resource used in determining the name, title, or other identifying
						attributes of an entity, or in determining the relationship between
					entities.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaStatusOfIdentification">
				<xs:annotation id="rdar-129">
					<xs:documentation>An indication of the level of authentication of the data identifying an
						entity.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="RDAStatusOfIdentification">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="FullyEstablished"/>
										<xs:enumeration value="Preliminary"/>
										<xs:enumeration value="Provisional"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaSummarizationOfTheContent">
				<xs:annotation id="rdar-130">
					<xs:documentation>An abstract, summary, synopsis, etc., of the content of a
					resource.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaSupplementaryContent">
				<xs:annotation id="rdar-131">
					<xs:documentation>Content (e.g., an index, a bibliography, an appendix) designed to
						supplement the primary content of a resource.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<!-- ======================================================================= -->
	<!--  RDA MANIFESTATION -->
	<!-- ======================================================================= -->
	<xs:complexType name="rdaManifestation">
		<xs:sequence>
			<xs:annotation>
				<xs:documentation>RDA conceptualization of FRBR Group 1 Manifestation Entity with
					accompanying RDA elements, subelements, and vocabularies. Where RDA defines Manifestation
					as: "The physical embodiment of an expression of a work."</xs:documentation>
			</xs:annotation>
			<xs:element name="rdaAppliedMaterial"/>
			<xs:element name="rdaCarrierType"/>
			<xs:element name="rdaCopyrightDate"/>
			<xs:element name="rdaDistributionStatement"/>
			<xs:element name="rdaExtent"/>
		    <xs:element name="rdaEditionStatement"/>
		    <xs:element name="rdaEditionStatement"/>
		    <xs:element name="rdaIdentifierForTheManifestation"/>
		    <xs:element name="rdaManufactureStatement"/>
		    <xs:element name="rdaNumberOfSerials"/>
		    <xs:element name="rdaPublicationStatement"/>
		    <xs:element name="rdaSeriesStatement"/>
		    <xs:element name="rdaStatementOfResponsibility"/>
		    <xs:element name="rdaTitle"/>
		</xs:sequence>
	</xs:complexType>

	<!-- ======================================================================= -->
	<!--  RDA ITEM -->
	<!-- ======================================================================= -->
	<xs:complexType name="rdaItem">
		<xs:sequence>
			<xs:annotation>
				<xs:documentation>RDA conceptualization of FRBR Group 1 Item Entity with accompanying RDA
					elements, subelements, and vocabularies. Where RDA defines Item as: "A single exemplar or
					instance of a manifestation."</xs:documentation>
			</xs:annotation>
			<xs:element name="rdaContactInformation">
				<xs:annotation id="rdar-150">
					<xs:documentation>Information relating to an organization, etc., from which a resource may
						be obtained.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaCustodialHistoryOfItem">
				<xs:annotation id="rdar-151">
					<xs:documentation>A record of previous ownership or custodianship of an
					item.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaDimensions">
				<xs:annotation id="rdar-152">
					<xs:documentation>The measurements of the carrier or carriers and/or the container of a
						resource.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="rdaDimensionsOfAMapetc.">
							<xs:annotation id="rdar-153">
								<xs:documentation>The measurements of the face of a map, etc.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="id" type="xs:ID"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="rdaDimensionsOfStillImage">
							<xs:annotation id="rdar-154">
								<xs:documentation>The measurements of the pictorial area of a still
								image.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="id" type="xs:ID"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="id" type="xs:ID"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="rdaExtent">
				<xs:annotation id="rdar-155">
					<xs:documentation>The number and type of units and/or subunits making up a
					resource.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="rdaExtentOfCartographicResource">
							<xs:annotation id="rdar-156">
								<xs:documentation>The number and type of units and/or subunits making up a
									cartographic resource.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:string">
										<xs:attribute name="RDAExtentOfCartographicResource">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="Atlas">
														<xs:annotation id="rdar-180">
															<xs:documentation>A volume of maps or other cartographic content, with
																or without descriptive text.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="Atlases">
														<xs:annotation id="rdar-181">
															<xs:documentation>Volumes of maps or other cartographic content, with
																or without descriptive text.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="Diagram"/>
													<xs:enumeration value="Diagrams"/>
													<xs:enumeration value="Globe">
														<xs:annotation id="rdar-182">
															<xs:documentation>A depiction of the Earth or other celestial body
																(real or imaginary) on the surface of a sphere.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="Globes">
														<xs:annotation id="rdar-183">
															<xs:documentation>Depictions of the Earth or other celestial body
																(real or imaginary) on the surface of a sphere.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="Map">
														<xs:annotation id="rdar-184">
															<xs:documentation>A representation, normally to scale and on a
																two-dimensional medium, of a selection of material or abstract
																features on, or in relation to, the surface of Earth, another
																celestial body, or an imaginary place.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="Maps">
														<xs:annotation id="rdar-185">
															<xs:documentation>Representations, normally to scale and on a
																two-dimensional medium, of a selection of material or abstract
																features on, or in relation to, the surface of Earth, another
																celestial body, or an imaginary place.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="Model">
														<xs:annotation id="rdar-186">
															<xs:documentation>1. A three-dimensional representation of a real or
																imagined object. 2. A three-dimensional representation of the whole
																or part of the Earth or any celestial body (real or imaginary) at
																any scale.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="Models">
														<xs:annotation id="rdar-187">
															<xs:documentation>1. Three-dimensional representations of a real or
																imagined object. 2. Three-dimensional representations of the whole
																or part of the Earth or any celestial body (real or imaginary) at
																any scale.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="Profile">
														<xs:annotation id="rdar-188">
															<xs:documentation>In cartography, a scale representation of the
																intersection of a vertical surface (which may or may not be a plane)
																with the surface of the ground, or of the intersection of such a
																vertical surface with that of a conceptual three-dimensional model
